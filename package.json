{
  "name": "live-lyrics",
  "productName": "Live Lyrics",
  "appname": "Live Lyrics",
  "version": "2.0.0",
  "description": "Live Lyrics",
  "build": {
    "appId": "com.electron.live-lyrics",
    "directories": {
      "buildResources": "build",
      "output": "dist"
    },
    "win": {
      "target": "nsis",
      "icon": "build/favicon.ico",
      "publish": ["github"]
    }
  },
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/iphren/live-lyrics.git"
  },
  "dependencies": {
    "express": "^4.17.1",
    "jquery": "^3.5.1",
    "pinyin": "^2.9.1",
    "socket.io": "^2.3.0",
    "sortablejs": "^1.10.2"
  },
  "devDependencies": {
    "electron": "^8.1.1",
    "electron-builder": "^22.7.0"
  }
}
